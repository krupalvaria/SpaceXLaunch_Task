{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [ ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        // "no-unused-vars": "error",
        "import/extensions": 0,
        // "import/no-extraneous-dependencies": ["error", {"packageDir": "./package.json"}],
        // "import/no-extraneous-dependencies": ["error", {"dependencies": false}],
        "no-unused-vars": ["error", { "argsIgnorePattern": "_" }],
        "no-return-await": 0,
        "no-underscore-dangle":0,
        "no-promise-executor-return":0,
        // "func-names": ["error", "as-needed"],
        "func-names": 0,
        "no-dupe-else-if": "error",
        "quotes": [ "error", "double" ],
        "linebreak-style": 0,
        "no-console":0,
        "space-unary-ops": "error",
        "multiline-ternary": ["error", "always-multiline"],
        "max-len": ["error", { "code": 200, "ignoreComments":true }],
        "key-spacing": ["error", { "beforeColon": false,"afterColon": true,"mode": "strict" }],
        "space-before-function-paren":"error",
        "object-curly-spacing": ["error", "always"],
        "comma-spacing": ["error", { "before": false, "after": true }],
        "object-curly-newline": [ "error", { "multiline": true, "consistent": true } ],
        "no-multi-spaces": ["error", { "ignoreEOLComments": true }],
        "indent": ["error", 2],
        "semi-spacing": [ "error", { "before": false, "after": true } ],
        "comma-dangle": [ "error", "never" ],
        "no-irregular-whitespace": [ "error", { "skipRegExps": true } ],
        "arrow-body-style": [ "error", "as-needed" ],
        "no-multiple-empty-lines": [ "error", { "max": 1, "maxEOF": 0 } ],
        "no-trailing-spaces": [ "error", { "ignoreComments": true } ],
        "require-yield": 0,
        "no-inner-declarations": 0,
        "semi": [ "error", "always" ],
        "no-empty": 0,
        "no-restricted-syntax": [ "error", "IfStatement > ExpressionStatement > AssignmentExpression", "WithStatement" ]
    }
}
